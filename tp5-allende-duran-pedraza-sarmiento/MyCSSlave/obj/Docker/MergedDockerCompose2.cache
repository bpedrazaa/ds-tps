services:
  mycsslave:
    build:
      context: C:\Users\Equipo\Documents\UPB\Semestre 7\Sistemas Distribuidos\ds-tps\tp5-allende-duran-pedraza-sarmiento\MyCSSlave
      dockerfile: Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MyCSSlave
      target: base
    container_name: MyCSSlave
    deploy:
      placement:
        constraints:
        - node.role == worker
      replicas: 1
      restart_policy:
        condition: on-failure
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
      SERVER: master
    image: 10.1.2.121:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/MyCSSlave.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5006
      target: 80
    - published: 50052
      target: 443
    tty: true
    volumes:
    - C:\Users\Equipo\Documents\UPB\Semestre 7\Sistemas Distribuidos\ds-tps\tp5-allende-duran-pedraza-sarmiento\MyCSSlave:/app:rw
    - C:\Users\Equipo\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Equipo\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Equipo\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Equipo\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\Equipo\Documents\UPB\Semestre 7\Sistemas Distribuidos\ds-tps\tp5-allende-duran-pedraza-sarmiento\MyCSSlave:/src:rw
version: '3.4'